<ul class="social">
	{% for key, url in site_data.contacts.social_accounts %}
		{% if url %}
			<li class="social__item">
				<a class="social__link" href="{{url}}" target="_blank">
					{% include 'components/icon.twig' with {
						name: key
					} %}
				</a>
			</li>
		{% endif %}
	{% endfor %}
</ul>