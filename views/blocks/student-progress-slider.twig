{% embed 'components/slider.twig' with {
    name: 'student-progress'
} %}
    {% block content %}
        {% for item in items %}
            <div class="swiper-slide">
                {% include 'blocks/progress-block.twig' with {
                    item
                } %}
            </div>
        {% endfor %}
    {% endblock %}
{% endembed %}